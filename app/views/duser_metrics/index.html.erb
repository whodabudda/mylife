<p id="notice"><%= notice %></p>

<h1>Listing Duser Metrics</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Occur dttm</th>
      <th>Duser</th>
      <th>Metric</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @duser_metrics.each do |duser_metric| %>
      <tr>
        <td><%= duser_metric.value %></td>
        <td><%= duser_metric.occur_dttm %></td>
        <td><%= duser_metric.duser %></td>
        <td><%= duser_metric.metric %></td>
        <td><%= link_to 'Show', duser_metric %></td>
        <td><%= link_to 'Edit', edit_duser_metric_path(duser_metric) %></td>
        <td><%= link_to 'Destroy', duser_metric, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Duser metric', new_duser_metric_path %>
